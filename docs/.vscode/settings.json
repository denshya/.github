{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "jsx",
    "tsx",
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "bemful",
    "chronly",
    "jsxfill",
    "payloadcms",
    "pinely"
  ]
}
